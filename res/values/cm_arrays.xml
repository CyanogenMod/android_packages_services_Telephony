    <!-- Preferred network mode list for slot1's roaming modes. -->
    <string-array name="roaming_mode_preferred_network_choices_slot1" translatable="false">
        <item>@array/roaming_mode1_preferred_network_choices</item>
        <item>@array/roaming_mode2_preferred_network_choices</item>
    </string-array>
    <string-array name="roaming_mode_preferred_network_values_slot1" translatable="false">
        <item>@array/roaming_mode1_preferred_network_values</item>
        <item>@array/roaming_mode2_preferred_network_values</item>
    </string-array>
    <!-- Roaming modes for slot 1, the preferred networks modes available for each roaming
         mode is read from the above list.
    -->
    <string-array name="roaming_mode_choices_slot1" translatable="false">
        <item>@string/roaming_gsm</item>
        <item>@string/roaming_cdma</item>
        <item>@string/roaming_disabled</item>
    </string-array>
    <!-- Index to look up preferred network list in roaming_mode_preferred_network_choices_slot1.
         Each mode must have a preferred network mode list except the special "Disabled" mode,
         which uses "-1"
    -->
    <string-array name="roaming_mode_values_slot1" translatable="false">
        <item>"0"</item>
        <item>"1"</item>
        <item>"-1"</item>
    </string-array>
    <!-- Warning message when user chooses the corresponding roaming mode. Set empty string
         if you don't need one.
    -->
    <string-array name="roaming_mode_warnings_slot1" translatable="false">
        <item>@string/roaming_warning</item>
        <item>@string/roaming_warning</item>
        <item>""</item>
    </string-array>
    <!-- Enable/disable a roaming mode. This allows us to overlay the settings.
         For example, we can allow a roaming mode only if a specific SIM cad is inserted,
         by setting the roaming_mode_enable_slotX to 0 (false) in this file, but overlaying
         it to "1" (true) in a the mccXXX-mncXXX xml file.
         When a mode is disabled, the setting UI only displays a warning dialog but does NOT
         allow any change (e.g. Please insert XXX SIM card to enable XXX roaming mode)
    -->
    <string-array name="roaming_mode_enable_slot1" translatable="false">
        <item>"1"</item>
        <item>"1"</item>
        <item>"1"</item>
    </string-array>

    <!-- Preferred network mode list for slot2's roaming modes. -->
