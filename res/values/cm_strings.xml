    <string name="blacklist_notification">Blocked a call from %s</string>
    <string name="blacklist_notification_private_number">Blocked a call from a private number</string>
    <string name="blacklist_notification_list_private">Private</string>
    <string name="blacklist_notification_unknown_number">Blocked unknown number %s</string>
    <string name="blacklist_notification_multiple"><xliff:g id="count">%d</xliff:g> calls were blocked</string>
    <string name="unblock_number">Unblock</string>

    <!-- Sprint MWI Quirk: Phantom message wait indicator workaround -->
    <string name="mwi_notification_title">Message wait indicator</string>
    <string name="mwi_notification_summary">Show message wait indicator voicemail notifications</string>

