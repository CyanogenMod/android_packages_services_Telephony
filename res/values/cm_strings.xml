    <string name="perm_change_mobile_network">change network mode (2G/3G)</string>
    <string name="perm_change_mobile_network_desc">Allows the app to change between 2G data and 3G data at any time. 3G is faster, but consumes more battery and may cause additional cost.</string>

    <!-- Noise suppression -->
    <string name="noise_suppression_title">Noise suppression</string>
    <string name="noise_suppression_summary">Enable noise suppression for calls</string>
